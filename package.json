{
  "name": "action-get-changed-files",
  "version": "1.2.0",
  "private": true,
  "description": "GitHub action to get the changes folders/files between the latest commit, and the previous latest commit",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run test && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stockopedia/action-get-changed-files.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^5.0.0",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "@types/minimatch": "^3.0.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.14.35",
    "@typescript-eslint/parser": "^4.29.3",
    "@vercel/ncc": "^0.29.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.1.5",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^26.6.3",
    "jest-circus": "^27.2.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5"
  }
}
