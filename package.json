{
  "name": "action-get-changed-files",
  "version": "1.2.0",
  "private": true,
  "description": "GitHub action to get the changes folders/files between the latest commit, and the previous latest commit",
  "main": "lib/main.js",
  "engines": {
    "node": "v16"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "run-s build format lint test package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stockopedia/action-get-changed-files.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "minimatch": "^5.0.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^16",
    "@typescript-eslint/parser": "^5.17.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.12.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.1.3",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.6.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
